{
  "hosting": {
    "public": "dist/COVID-Dashboard",
    "target": "COVID-Dashboard",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "y"
  }
}
